{
  "name": "@dtrw/eslint-config",
  "version": "2.0.0-alpha.7",
  "author": "Bartosz Dotryw <burtekdotryw@gmail.com>",
  "description": "Custom eslint configs",
  "keywords": [
    "eslint",
    "config",
    "eslint config",
    "typescript",
    "json"
  ],
  "license": "MIT",
  "dependencies": {
    "@next/eslint-plugin-next": "^13.4.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint-define-config": "^1.20.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsonc": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-security-node": "^1.1.1",
    "globals": "^13.20.0",
    "husky": "^8.0.3",
    "jsonc-eslint-parser": "^2.3.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.40.0",
    "exit-code": "^1.0.2",
    "typescript": "~5.0.4"
  },
  "peerDependencies": {
    "eslint": ">=8.23",
    "typescript": ">=4"
  },
  "private": false,
  "scripts": {
    "lint": "eslint {configs,rules}/**/*.js",
    "test": "node tests/run-tests.js",
    "preversion-temp-disabled": "yarn test",
    "push": "git push --follow-tags",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "homepage": "https://github.com/burtek/eslint-config#readme",
  "bugs": "https://github.com/burtek/eslint-config/issues",
  "repository": {
    "url": "https://github.com/burtek/eslint-config"
  },
  "engines": {
    "node": ">=14"
  },
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "auto-changelog": {
    "breakingPattern": "BREAKING CHANGE:",
    "unreleased": true,
    "commitLimit": false
  }
}
